{
  "name": "nextstax",
  "version": "1.0.1",
  "description": "An npm package for deploying and managing web projects.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NextStax/nextstax.git"
  },
  "keywords": [
    "nextstax",
    "nextstax-sdk"
  ],
  "author": "Kenny Stanley (https://github.com/KennyStanley)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/NextStax/nextstax/issues"
  },
  "homepage": "https://github.com/NextStax/nextstax#readme",
  "devDependencies": {
    "@types/fetch-mock": "^7.3.5",
    "@types/jest": "^27.0.2",
    "axios-mock-adapter": "^1.20.0",
    "dotenv": "^10.0.0",
    "fetch-mock": "^9.11.0",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
